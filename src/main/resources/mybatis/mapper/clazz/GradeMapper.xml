<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zxd.www.clazz.mapper.GradeMapper">

    <!-- List<GradeEntity> selectList(); -->
    <select id="selectList" resultType="com.zxd.www.clazz.entity.GradeEntity" >
        select `grade_id`, `grade_name`
        from `t_grade`
        where `removed` = 0
    </select>


</mapper>